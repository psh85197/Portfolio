{
  "elements": ["html5"],
  "extends": ["html-validate:recommended"],
  "rules": {
    "doctype-style": "off",
    "prefer-native-element": "off",
    "no-inline-style": "off",
    "no-dup-id": "off",
    "form-dup-name": "off",
    "unique-landmark": "off",
    "wcag/h63": "off",
    "void-style": ["error", { "style": "selfclosing" }]
  }
}